<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="@dimen/media_progress_corners"/>
            <gradient android:startColor="@color/media_progress_background" android:endColor="@color/media_progress_background" android:angle="270" android:centerY="0.75" android:centerColor="@color/media_progress_background"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="@dimen/media_progress_corners"/>
                <gradient android:startColor="@color/media_progress" android:endColor="@color/media_progress" android:angle="270" android:centerY="0.75" android:centerColor="@color/media_progress"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="@dimen/media_progress_corners"/>
                <gradient android:startColor="@color/media_progress" android:endColor="@color/media_progress" android:angle="270" android:centerY="0.75" android:centerColor="@color/media_progress"/>
            </shape>
        </clip>
    </item>
</layer-list>
